{
  "name": "FitnessApp",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "11",
    "npm": "6"
  },
  "scripts": {
    "start": "react-native start",
    "android:run:debug": "react-native run-android",
    "android:run:release": "react-native run-android --variant=release",
    "android:release": "npm run android:prod-bundle && cd android && ./gradlew clean bundleRelease -x bundleReleaseJsAndAssets --stacktrace",
    "log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "emulator": "emulator -read-only -no-boot-anim -avd 10.1_WXGA_Tablet_API_28",
    "emulator:menu": "adb shell input keyevent 82",
    "android:dev-bundle": "react-native bundle --platform android --dev true --entry-file index.js --assets-dest android/app/src/main/res --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:prod-bundle": "react-native bundle --platform android --dev false --entry-file index.js --assets-dest android/app/src/main/res --bundle-output android/app/src/main/assets/index.android.bundle",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.4.2",
    "i18n-js": "^3.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "random": "^2.1.1",
    "react": "16.6.3",
    "react-native": "^0.59.9",
    "react-native-chart-kit": "^2.6.1",
    "react-native-cli": "^2.0.1",
    "react-native-datepicker": "^1.7.2",
    "react-native-fs": "^2.13.3",
    "react-native-iap": "^2.5.5",
    "react-native-localize": "^1.1.3",
    "react-native-navigation": "^2.21.0",
    "react-native-responsive-grid": "^0.41.992",
    "react-native-scalable-image": "^0.5.1",
    "react-native-svg": "^9.4.0",
    "react-native-ui-lib": "^3.27.0",
    "react-native-vector-icons": "^6.4.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
    "react-test-renderer": "16.6.3",
    "uuid": "^3.3.2"
  }
}
